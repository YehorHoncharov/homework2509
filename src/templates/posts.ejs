<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><% tilte= posts %></title>
    <link rel="stylesheet" href="/static/style.css">
    <script defer src="/static/posts.js"></script>
</head>
<body>
    <div class="container">
        <div class="post">
            <h1>POST 1</h1>
            <% posts.forEach((post) => { %>
                <h2><%= post.name1 %></h2>
                <p><%= post.author1 %></p>
            <% }) %>
        </div>
    </div>

    <div class="container">
        <div class="post">
            <h1>POST 2</h1>
            <% posts.forEach((post) => { %>
                <h2><%= post.name2 %></h2>
                <p><%= post.author2 %></p>
            <% }) %>
        </div>  
    </div>
</body>
</html>