model Post {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  src         String
  author      String
  date        String

  comments Comment[]

  User   User? @relation(fields: [userId], references: [id])
  userId Int?

  Category Category? @relation(fields: [categoryId], references: [id])
  categoryId Int?
}
